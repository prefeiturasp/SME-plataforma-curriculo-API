<%= semantic_form_for(
  challenge, :url => "/", builder: ActiveAdmin::FormBuilder
) do |new_form| %>
<% ContentBlock.human_attribute_content_blocks.each do |name, key|
  next unless [:gallery, :free_text, :question, :open_text, :bullet].include?(key.to_sym)
%>
